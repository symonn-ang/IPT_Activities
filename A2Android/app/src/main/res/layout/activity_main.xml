<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="67dp"
        android:layout_height="66dp"
        app:srcCompat="@drawable/yep4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:fontFamily="@font/kaushan_script"
        android:text="Instagraph"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@id/imageView"
        app:layout_constraintStart_toEndOf="@id/imageView"
        app:layout_constraintTop_toTopOf="@id/imageView"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="67dp"
        android:layout_height="66dp"
        app:srcCompat="@drawable/not1"
        app:layout_constraintTop_toTopOf="@id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="16dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="___________________________"
        android:textSize="34sp"
        app:layout_constraintTop_toBottomOf="@id/imageView"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="8dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/circleImageView1"
        android:layout_width="80dp"
        android:layout_height="81dp"
        android:scaleType="centerCrop"
        android:src="@drawable/yep8"
        app:shapeAppearanceOverlay="@style/CircleImageView"
        app:layout_constraintTop_toBottomOf="@id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="8dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/circleImageView2"
        android:layout_width="80dp"
        android:layout_height="81dp"
        android:scaleType="centerCrop"
        android:src="@drawable/yep7"
        app:shapeAppearanceOverlay="@style/CircleImageView"
        app:strokeColor="#FF4081"
        app:strokeWidth="3dp"
        app:layout_constraintTop_toTopOf="@id/circleImageView1"
        app:layout_constraintStart_toEndOf="@id/circleImageView1"
        android:layout_marginStart="16dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/circleImageView3"
        android:layout_width="80dp"
        android:layout_height="81dp"
        android:scaleType="centerCrop"
        android:src="@drawable/yep9"
        app:shapeAppearanceOverlay="@style/CircleImageView"
        app:strokeColor="#FF4081"
        app:strokeWidth="3dp"
        app:layout_constraintTop_toTopOf="@id/circleImageView1"
        app:layout_constraintStart_toEndOf="@id/circleImageView2"
        android:layout_marginStart="16dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/circleImageView4"
        android:layout_width="80dp"
        android:layout_height="81dp"
        android:scaleType="centerCrop"
        android:src="@drawable/yep10"
        app:shapeAppearanceOverlay="@style/CircleImageView"
        app:strokeColor="#FF4081"
        app:strokeWidth="3dp"
        app:layout_constraintTop_toTopOf="@id/circleImageView1"
        app:layout_constraintStart_toEndOf="@id/circleImageView3"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="___________________________"
        android:textSize="34sp"
        app:layout_constraintTop_toBottomOf="@id/circleImageView1"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="8dp" />

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/circleImageView5"
        android:layout_width="70dp"
        android:layout_height="71dp"
        android:scaleType="centerCrop"
        android:src="@drawable/yep10"
        app:shapeAppearanceOverlay="@style/CircleImageView"
        app:strokeColor="#000000"
        app:strokeWidth="3dp"
        app:layout_constraintTop_toBottomOf="@id/textView3"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="27dp"
        android:layout_marginTop="8dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Brad Pitt"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@id/circleImageView5"
        app:layout_constraintTop_toTopOf="@id/circleImageView5"
        android:layout_marginStart="16dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" @bradpitt45"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@id/textView4"
        app:layout_constraintTop_toBottomOf="@id/textView4"
        android:layout_marginTop="4dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0dp"
        android:layout_height="343dp"
        app:srcCompat="@drawable/yep11"
        app:layout_constraintTop_toBottomOf="@id/circleImageView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="8dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="207dp"
        android:layout_height="38dp"
        app:srcCompat="@drawable/yep13"
        app:layout_constraintTop_toBottomOf="@id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginStart="10dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3400 likes"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/imageView5"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginStart="15dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kanye:"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="15dp"
        android:layout_marginTop="4dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Yo wassup"
        android:textSize="20sp"
        app:layout_constraintTop_toTopOf="@id/textView8"
        app:layout_constraintStart_toEndOf="@id/textView8"
        android:layout_marginStart="4dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=". . ."
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/textView8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="8dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
